<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <Nullable>enable</Nullable>
        <LangVersion>9</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Autofac" Version="6.3.0" />
      <PackageReference Include="DiffMatchPatchCSharp" Version="1.0.0" />
      <PackageReference Include="Discord.Net.Labs" Version="3.1.7" />
      <PackageReference Include="Lavalink4NET" Version="2021.10.6.1409" />
      <PackageReference Include="Lavalink4NET.Discord.NET" Version="2021.10.6.1409" />
      <PackageReference Include="Lavalink4NET.MemoryCache" Version="2021.10.6.1409" />
      <PackageReference Include="Lib.Harmony" Version="2.1.1" />
      <PackageReference Include="LiteDB" Version="5.0.11" />
      <PackageReference Include="NLog" Version="4.7.12" />
      <PackageReference Include="System.Reactive" Version="5.0.0" />
      <PackageReference Include="Tyrrrz.Extensions" Version="1.6.5" />
      <PackageReference Include="YandexMusicResolver" Version="4.0.0" />
    </ItemGroup>

    <ItemGroup>
      <None Update="Localization\ru.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="Localization\en.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="Localization\es.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
    </ItemGroup>
</Project>
