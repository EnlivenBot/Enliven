<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Bot</RootNamespace>
    <AssemblyName>Enliven</AssemblyName>
    <Version>16.0</Version>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.1.0" />
    <PackageReference Include="Autofac.Extras.NLog" Version="4.0.0" />
    <PackageReference Include="GrapeCity.Documents.Html" Version="3.2.0.556" />
    <PackageReference Include="GrapeCity.Documents.Html.Linux.X64" Version="3.2.0.556" />
    <PackageReference Include="GrapeCity.Documents.Html.Mac.X64" Version="3.2.0.556" />
    <PackageReference Include="GrapeCity.Documents.Html.Windows.X64" Version="3.2.0.556" />
    <PackageReference Include="GrapeCity.Documents.Imaging" Version="3.2.0.556" />
    <PackageReference Include="Lib.Harmony" Version="2.0.4" />
    <PackageReference Include="LiteDB" Version="5.0.10" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NLog" Version="4.7.6" />
    
    <PackageReference Include="Polly" Version="7.2.1" />
    <PackageReference Include="Scriban" Version="3.3.3" />
    <PackageReference Include="SpotifyAPI.Web" Version="6.0.0" />
    <PackageReference Include="System.Reactive" Version="5.0.0" />
    <PackageReference Include="Tyrrrz.Extensions" Version="1.6.5" />
    <PackageReference Include="YandexMusicResolver" Version="4.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Localization\en.json" />
    <None Update="nlog.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="DiscordChatExporter.Domain, Version=2.20.0.0, Culture=neutral, PublicKeyToken=null">
      <HintPath>..\packages\DiscordChatExporter.Domain.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Common\Common.csproj" />
  </ItemGroup>

</Project>