{
  "SpotifyCredentials": {
    "SpotifyClientID": null,
    "SpotifyClientSecret": null
  },
  "YandexCredentials": {
    "Token": null,
    "Login": null,
    "Password": null
  },
  "VkCredentials": {
    "AccessToken": null
  },
  "ApiExternalUrl": "http://localhost:8088",
  "LavalinkNodes": [],
  "Instances": [],
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug",
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{@t:HH:mm:ss} {@l:u3}] {#if InstanceName is not null}[{InstanceName}] {#end}{SourceContext} | {@m}\n{#if @x is not null} {@x}\n{#end}",
            "theme": "Serilog.Templates.Themes.TemplateTheme::Code, Serilog.Expressions"
          }
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/latest-detailed.log",
          "restrictedToMinimumLevel": "Debug",
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{@t:HH:mm:ss} {@l:u3}] {#if InstanceName is not null}[{InstanceName}] {#end}{SourceContext} | {@m}\n{#if @x is not null} {@x}\n{#end}",
            "theme": "Serilog.Templates.Themes.TemplateTheme::Code, Serilog.Expressions"
          },
          "rollingInterval": "Day",
          "retainedFileCountLimit": 1
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/latest.log",
          "restrictedToMinimumLevel": "Information",
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{@t:HH:mm:ss} {@l:u3}] {#if InstanceName is not null}[{InstanceName}] {#end}{SourceContext} | {@m}\n{#if @x is not null} {@x}\n{#end}",
            "theme": "Serilog.Templates.Themes.TemplateTheme::Code, Serilog.Expressions"
          },
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31,
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 104857600
        }
      }
    ],
    "CustomLogLevel": {
      "System.Net.Http.HttpClient": "Fatal",
      "Polly": "Fatal",
      "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Error"
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": {
          "maximumDestructuringDepth": 4
        }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": {
          "maximumStringLength": 100
        }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": {
          "maximumCollectionCount": 10
        }
      }
    ]
  }
}